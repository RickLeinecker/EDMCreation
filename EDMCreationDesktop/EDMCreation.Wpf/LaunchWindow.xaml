<views:MvxWindow x:Class="EDMCreation.Wpf.LaunchWindow"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:local="clr-namespace:EDMCreation.Wpf"
                 xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
                 xmlns:vw="clr-namespace:EDMCreation.Wpf.Views"
                 xmlns:vm="clr-namespace:EDMCreation.Core.ViewModels;assembly=EDMCreation.Core"
                 mc:Ignorable="d"
        Title="EDM Creation" Height="450" Width="800">
    <Window.Resources>
        <DataTemplate DataType="{x:Type vm:CreateAccountViewModel}">
            <vw:CreateAccountView DataContext="{Binding}" />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:LoginViewModel}">
            <vw:LoginView DataContext="{Binding}" />
        </DataTemplate>
    </Window.Resources>
    <Grid>
    </Grid>
</views:MvxWindow>
